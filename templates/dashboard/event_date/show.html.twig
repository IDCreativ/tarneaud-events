{% extends 'admin.html.twig' %}

{% block title %}EventDate{% endblock %}

{% block body %}
    <h1>EventDate</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ event_date.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ event_date.name }}</td>
            </tr>
            <tr>
                <th>DateStart</th>
                <td>{{ event_date.dateStart ? event_date.dateStart|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateEnd</th>
                <td>{{ event_date.dateEnd ? event_date.dateEnd|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('event_date_index') }}">back to list</a>

    <a href="{{ path('event_date_edit', {'id': event_date.id}) }}">edit</a>

    {{ include('dashboard/event_date/_delete_form.html.twig') }}
{% endblock %}
