<form class="d-inline" method="post" action="{{ path('poll_delete', {'id': poll.id}) }}" onsubmit="return confirm('Êtes-vous certain de vouloir supprimer cet élément ? Cette action est irréversible !);">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ poll.id) }}">
    <button class="btn btn-outline-danger"><i class="fal fa-trash-alt"></i></button>
</form>
